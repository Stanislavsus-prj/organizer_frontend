{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "max-len": "off",
    "no-unused-expressions": "off",
    "array-callback-return": "off",
    "no-prototype-builtins": "off",
    "class-methods-use-this": "off",
    "consistent-return": "off",
    "no-alert": "off",
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}